{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				"navigationStyle": "custom",  // 使用自定义导航栏
				"enablePullDownRefresh": true,  // 启用下拉刷新
                "backgroundTextStyle": "dark",  // 下拉刷新loading样式
                "backgroundColor": "#f8f8f8"    // 下拉刷新背景色
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "和喜安筑",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"navigationStyle": "custom", // 全局启用自定义导航栏
		"rpxCalcMaxDeviceWidth": 750, // 设计稿宽度
		"rpxCalcBaseDeviceWidth": 375, // 开发设备宽度
		"rpxCalcIncludeWidth": 750
	},
	"uniIdRouter": {},
	"easycom": {
    	"autoscan": true,
		"custom": {
			"^uni-(.*)": "@/uni_modules/uni-$1/uni-$1.vue"
		}
	}
}